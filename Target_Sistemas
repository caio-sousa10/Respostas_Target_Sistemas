 5) using System;

class stringinvertida
{
    static void Main(string[] args)
    {
        Console.Write("Digite a string: ");
        string str = Console.ReadLine();

        string invertidaStr = "";
        for (int i = str.Length - 1; i >= 0; i--)
        {
            invertidaStr += str[i];
        }

        Console.WriteLine("A String ficou: " + invertidaStr);
    }
}
