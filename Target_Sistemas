2) internal class Program
{
    private static void Main(string[] args)
    {
        int i = 0;
        int a = 0, n = 1, c = 1;
        c = a + n; a = n; n = c;
        i++;
        Console.Write("Digite um número: ");
        _ = Convert.ToInt32(Console.ReadLine());
        if (IsFibonacci(n)) Console.WriteLine("{0} faz parte da sequência de Fibonacci.", n);
        else Console.WriteLine("{0} não faz parte da sequência de Fibonacci.", n);
        while (i < 10)
        {
            c = a + n; a = n; n = c;
            i++;
            Console.WriteLine("A sequência é " + (c));
        }

    }
    static bool IsFibonacci(int n)
    {
        if (n == 0 || n == 1)
            return true;
        int a = 0, b = 1, c = 1;
        while (c < n)
        {
            c = a + b; a = b; b = c;
        }
        return c == n;
    }
}

